Cell[TextData[{
 "If, other than the degree, you don\[CloseCurlyQuote]t care about the \
particular form of the polynomial, then the solution is fine. However, if you \
want to specify the coefficients, you can generate a polynomial with ",
 StyleBox["Dot", "Literal"],
 "."
}], "Text",
 CellChangeTimes->{{3.471027043639477*^9, 3.471027065309441*^9}, 
   3.471028143863154*^9}],

Cell[BoxData[{
 RowBox[{"ClearAll", "[", "makePoly", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"SetAttributes", "[", 
   RowBox[{"makePoly", ",", "HoldRest"}], "]"}], ";", 
  RowBox[{
   RowBox[{"makePoly", "[", 
    RowBox[{"coef_List", ",", 
     RowBox[{"var_:", "x"}]}], "]"}], " ", ":=", " ", 
   RowBox[{"Dot", "[", 
    RowBox[{
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"var", "^", "i"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "0", ",", 
         RowBox[{
          RowBox[{"Length", "[", "coef", "]"}], "-", "1"}]}], "}"}]}], "]"}], 
     ",", "coef"}], "]"}]}]}]}], "Input"],

