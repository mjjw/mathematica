          RowBox[{"Length", "[", "coef", "]"}], "-", "1"}], ",", "0", ",", 
         RowBox[{"-", "1"}]}], "}"}]}], "]"}], ",", "coef", ",", 
     RowBox[{
      RowBox[{"HoldForm", "[", 
       RowBox[{"Plus", "[", "##", "]"}], "]"}], "&"}]}], "]"}]}]}]}], "Input"],

