Cell[CellGroupData[{

Cell["Solution", "Heading2"],

Cell[TextData[{
 "Use ",
 StyleBox["Solve", "Literal"],
 " with expressions of the form ",
 StyleBox["left-hand-side", "UserInput"],
 " ",
 StyleBox["== right-hand-side", "UserInput"],
 " and the unknown variable (or variables) provided as the second argument. \
Results are returned as rules."
}], "Text",
 CellChangeTimes->{
  3.4710255379134493`*^9, {3.475943150693185*^9, 3.475943151203703*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"x", "^", "2"}], " ", "-", " ", 
     RowBox[{"2", "x"}], " ", "-", " ", "3"}], " ", "\[Equal]", " ", "0"}], 
   ",", "x"}], "]"}]], "Input"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"x", "\[Rule]", 
     RowBox[{"-", "1"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", "\[Rule]", "3"}], "}"}]}], "}"}]], "Output",
 CellChangeTimes->{3.4673686508761997`*^9}]
}, Open  ]],

Cell[TextData[{
 StyleBox["Solve", "Literal"],
 " takes either a single expression, as above, or a list of expressions or \
several expressions linked with ",
 StyleBox["&&", "Literal"],
 ". As you would expect, solutions can be found in symbolic form."
}], "Text",
 CellChangeTimes->{{3.471025548696982*^9, 3.47102556117411*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"a", " ", "x"}], "+", 
       RowBox[{"2", "y"}]}], "\[Equal]", "7"}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"3", " ", "b", " ", "x"}], "-", "y"}], "\[Equal]", "1"}]}], 
    "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "y"}], "}"}]}], "]"}]], "Input"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"x", "\[Rule]", 
     FractionBox["9", 
      RowBox[{"a", "+", 
       RowBox[{"6", " ", "b"}]}]]}], ",", 
    RowBox[{"y", "\[Rule]", 
     RowBox[{"-", 
      FractionBox[
       RowBox[{"a", "-", 
        RowBox[{"21", " ", "b"}]}], 
       RowBox[{"a", "+", 
        RowBox[{"6", " ", "b"}]}]]}]}]}], "}"}], "}"}]], "Output",
 CellChangeTimes->{3.4673686509972*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
