Cell[TextData[{
 "Mathematica also provides ",
 StyleBox["PolynomialMod", "Literal"],
 ", which uses an algorithm based on repeated subtraction and, hence, never \
performs a division. In contrast, ",
 StyleBox["Polynomial\[DiscretionaryHyphen]Remainder", "Literal"],
 " is implemented in terms of ",
 StyleBox["PolynomialQuotient", "Literal"],
 "."
}], "Text",
 CellChangeTimes->{{3.471027588942618*^9, 3.4710275941113377`*^9}, 
   3.471027789456402*^9, {3.4710295271629667`*^9, 3.4710295287759457`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"With", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"poly", " ", "=", 
      RowBox[{
       RowBox[{"x", "^", "3"}], "+", 
       RowBox[{"x", "^", "2"}], "-", "x", "+", "1"}]}], ",", " ", 
     RowBox[{"divisor", " ", "=", " ", 
      RowBox[{"x", "^", "2"}]}]}], "}"}], ",", 
   RowBox[{
    RowBox[{"Simplify", "[", 
     RowBox[{"poly", "-", 
      RowBox[{"(", 
       RowBox[{"divisor", "*", " ", 
        RowBox[{"PolynomialQuotient", "[", 
         RowBox[{"poly", ",", "divisor", ",", "x"}], "]"}]}], " ", ")"}]}], 
     "]"}], " ", "===", 
    RowBox[{"PolynomialRemainder", "[", 
     RowBox[{"poly", ",", "divisor", ",", "x"}], "]"}]}]}], "]"}]], "Input"],

Cell[BoxData["True"], "Output",
 CellChangeTimes->{3.4673686935202*^9}]
}, Open  ]],

