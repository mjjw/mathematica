     RowBox[{"var_:", "x"}]}], "]"}], " ", ":=", " ", 
   RowBox[{"Inner", "[", 
    RowBox[{"Times", ",", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"var", "^", "i"}], ",", 
       RowBox[{"{", 
