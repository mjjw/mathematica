Cell[TextData[{
 "It is also easy to detect if a matrix is a row permutation of another \
matrix: simply remove each row from ",
 StyleBox["m1", "Literal"],
 " that matches ",
 StyleBox["m2", "Literal"],
 " and see if you are left with no rows. Of course, you must also check that \
the matrices are the same size. A check for column permutation is just a \
check for row permutations on the transpose of each matrix."
}], "Text",
 CellChangeTimes->{{3.41986852825*^9, 3.419868728890625*^9}, {
  3.420104742859375*^9, 3.42010474796875*^9}, {3.469984205544478*^9, 
  3.469984228617413*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"isRowPermutation", "[", 
   RowBox[{"m1_", ",", "m2_"}], "]"}], " ", ":=", " ", 
  RowBox[{
   RowBox[{
    RowBox[{"Length", "[", "m1", "]"}], " ", "\[Equal]", " ", 
    RowBox[{"Length", "[", "m2", "]"}]}], " ", "&&", " ", 
   RowBox[{
    RowBox[{"Fold", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"DeleteCases", "[", 
        RowBox[{"#1", ",", "#2"}], "]"}], "&"}], ",", "m1", ",", "m2"}], 
     "]"}], " ", "\[Equal]", " ", 
    RowBox[{"{", "}"}]}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"isMatrixPermutation", "[", 
   RowBox[{"m1_", ",", "m2_"}], "]"}], " ", ":=", " ", 
  RowBox[{
   RowBox[{"isRowPermutation", "[", 
    RowBox[{"m1", ",", "m2"}], "]"}], " ", "||", " ", 
   RowBox[{"isRowPermutation", "[", 
    RowBox[{
     RowBox[{"Transpose", "[", "m1", "]"}], ",", 
     RowBox[{"Transpose", "[", "m2", "]"}]}], "]"}]}]}]}], "Input",
