 StyleBox["Properties of graphs can be tested using a variety of functions, \
such as ",
  FontTracking->"SemiCondensed"],
 StyleBox["Hamilton\[DiscretionaryHyphen]ianQ", "Literal",
  FontTracking->"SemiCondensed"],
 StyleBox[" ",
  FontTracking->"SemiCondensed"],
 StyleBox["(which has a cycle that visits each vertex once), ",
  FontTracking->"Plain"],
 StyleBox["EulerianQ", "Literal",
  FontTracking->"Plain"],
 StyleBox[" (which has a tour that",
  FontTracking->"Plain"],
 " ",
 StyleBox["traverses each edge once), ",
  FontTracking->"Plain"],
 StyleBox["AntisymmetricQ", "Literal",
  FontTracking->"Plain"],
 StyleBox[", ",
  FontTracking->"Plain"],
 StyleBox["ReflexiveQ", "Literal",
  FontTracking->"Plain"],
 StyleBox[", ",
  FontTracking->"Plain"],
