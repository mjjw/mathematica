  FontTracking->"Plain"],
 StyleBox[", ",
  FontTracking->"Plain"],
 StyleBox["Self", "Literal",
  FontTracking->"Plain"],
 StyleBox["\[InvisibleSpace]",
  FontSize->0,
  FontTracking->"Plain"],
