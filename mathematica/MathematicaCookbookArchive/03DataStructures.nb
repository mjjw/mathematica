   RowBox[{"list_", ",", "k_"}], "]"}], " ", ":=", " ", 
  RowBox[{"SparseArray", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Band", "[", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"k", ">", "0"}], ",", 
        RowBox[{"{", 
         RowBox[{"1", ",", 
          RowBox[{"1", "+", "k"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"1", "-", "k"}], ",", "1"}], "}"}]}], "]"}], "]"}], 
     "\[Rule]", "list"}], ",", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Length", "[", "list", "]"}], "+", 
       RowBox[{"Abs", "[", "k", "]"}]}], ")"}], 
     RowBox[{"{", 
      RowBox[{"1", ",", "1"}], "}"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.420060406546875*^9, 3.420060432453125*^9}},
