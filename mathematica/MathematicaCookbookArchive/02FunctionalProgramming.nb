Cell["Solution", "Heading2"],

Cell[TextData[{
 "Use ",
 StyleBox["Hold", "Literal"],
 " in the argument list. Here I create a function called ",
 StyleBox["arrayAssign", "Literal"],
 " whose objective is to accept a symbol that is associated with a list, an \
index (or ",
 StyleBox["Span", "Literal"],
 "), and a second symbol associated with another list. The result is the \
assignment of the elements of ",
 StyleBox["array2", "Literal"],
 " to ",
 StyleBox["array1", "Literal"],
 " that are specified by index. For this to work, arguments ",
 StyleBox["a", "Literal"],
 " and ",
 StyleBox["b", "Literal"],
 " must remain held but ",
 StyleBox["aIndex", "Literal"],
 " should not."
}], "Text",
 CellChangeTimes->{{3.4699682371523943`*^9, 3.469968239758521*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"array1", " ", "=", " ", 
   RowBox[{"Table", "[", 
    RowBox[{"0", ",", " ", 
     RowBox[{"{", "10", "}"}]}], "]"}]}], ";", " ", 
  RowBox[{"array2", " ", "=", " ", 
   RowBox[{"Table", "[", 
    RowBox[{"1", ",", " ", 
     RowBox[{"{", "10", "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"arrayAssign", "[", 
    RowBox[{
     RowBox[{"Hold", "[", "a_Symbol", "]"}], ",", "aIndex_", ",", 
     RowBox[{"Hold", "[", "b_Symbol", "]"}], ",", "bIndex_"}], "]"}], ":=", 
   " ", "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"a", "[", 
        RowBox[{"[", "aIndex", "]"}], "]"}], " ", "=", " ", 
       RowBox[{"b", "[", 
        RowBox[{"[", "bIndex", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"a", "[", 
       RowBox[{"[", "aIndex", "]"}], "]"}]}]}], "]"}]}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "Assign", " ", "elements", " ", "2", " ", "through", " ", "3", " ", "in", 
    " ", "array", " ", "2", " ", "to", " ", "array", " ", "1."}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"arrayAssign", "[", 
   RowBox[{
    RowBox[{"Hold", "[", "array1", "]"}], ",", 
    RowBox[{"2", ";;", "3"}], ",", 
    RowBox[{"Hold", "[", "array2", "]"}], ",", "1"}], "]"}], 
  ";"}], "\[IndentingNewLine]", "array1"}], "Input",
