Cell[TextData[{
 "Mathematica does not support implicit currying like Haskell does, but you \
can use this solution to create functions that curry implicitly. Refer to ",
 ButtonBox["the next section, \[OpenCurlyDoubleQuote]Discussion,\
\[CloseCurlyDoubleQuote]",
  BaseStyle->"Hyperlink",
  ButtonData->"Discussion2.15"],
 " if you are unfamiliar with currying."
}], "Text",
 CellChangeTimes->{{3.469992318844839*^9, 3.469992319841653*^9}, {
  3.469992681611946*^9, 3.469992716053164*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{"f", ",", "f1", ",", "f2"}], "]"}], ";", 
  RowBox[{
   RowBox[{"Curry", "[", 
    RowBox[{"f_", ",", "x__"}], "]"}], " ", ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "expr", "}"}], ",", 
     RowBox[{
      RowBox[{"expr", " ", "=", " ", 
       RowBox[{
        RowBox[{
         RowBox[{"Hold", "[", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"ValueQ", "[", 
             RowBox[{"f", "[", "x", "]"}], "]"}], ",", 
            RowBox[{"f", "[", "x", "]"}], ",", 
            RowBox[{
             RowBox[{"Curry", "[", 
              RowBox[{"z", ",", "##"}], "]"}], "&"}]}], "]"}], "]"}], "//.", 
         RowBox[{
          RowBox[{
           RowBox[{"g_", "[", "a_", "]"}], "[", "b__", "]"}], "->", 
          RowBox[{"g", "[", 
           RowBox[{"a", ",", "b"}], "]"}]}]}], " ", "/.", 
        RowBox[{"z", "\[Rule]", 
         RowBox[{"f", "[", "x", "]"}]}]}]}], ";", 
      RowBox[{"ReleaseHold", "[", "expr", "]"}]}]}], 
    "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"f", "[", 
   RowBox[{"x_", ",", "y_", ",", "z_"}], "]"}], " ", ":=", " ", 
  RowBox[{"x", " ", "+", " ", "y", " ", "+", " ", "z"}]}]}], "Input",
