   3.469992138486045*^9, 3.4699921904322433`*^9}, 3.470055877545354*^9, {
   3.470147921608695*^9, 3.4701479465587482`*^9}, {3.470147995644928*^9, 
   3.4701479994270067`*^9}, {3.4701480408812237`*^9, 3.47014804101396*^9}, {
   3.470148190390441*^9, 3.4701482027940397`*^9}, 3.470156332335952*^9, 
   3.4704908126098146`*^9, {3.4704909978169947`*^9, 3.470491010711892*^9}, {
   3.470491064963728*^9, 3.47049108700671*^9}, 3.485522301467708*^9}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "First", " ", "define", " ", "a", " ", "closure", " ", "with", " ", "a", 
    " ", "single", " ", "function", " ", "and", " ", "assign", " ", "to", " ",
     "a", " ", 
    RowBox[{"variable", "."}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{"incr", " ", "=", " ", 
   RowBox[{"closure", "[", 
    RowBox[{
     RowBox[{"{", "x", "}"}], ",", " ", 
     RowBox[{"{", "0", "}"}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"incr", " ", "=", " ", 
       RowBox[{"Function", "[", 
        RowBox[{"x", " ", "=", " ", 
         RowBox[{"x", " ", "+", " ", "1"}]}], "]"}]}], "}"}]}], 
    "]"}]}]}]], "Input",
 CellChangeTimes->{3.469992200454043*^9},
 CellLabel->"In[255]:="],

Cell[BoxData[
 RowBox[{"closure", "[", 
  RowBox[{
   RowBox[{"{", "x", "}"}], ",", 
   RowBox[{"{", "0", "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"incr", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"x", "=", 
        RowBox[{"x", "+", "1"}]}], ")"}], "&"}]}], "}"}]}], "]"}]], "Output",
 CellChangeTimes->{3.4656840662139997`*^9, 3.4685945848464622`*^9},
 CellLabel->"Out[255]="]
}, Open  ]],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Then", " ", "define", " ", "a", " ", "function", " ", "pattern", " ", 
    "in", " ", "terms", " ", "of", " ", "the", " ", "same", " ", "closure", 
    " ", "but", " ", "with", " ", "a", " ", "Blank", " ", "where", " ", "the",
     " ", "state", " ", "variables", " ", "would", " ", 
    RowBox[{"reside", "."}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"closure", "[", 
     RowBox[{
      RowBox[{"{", "x", "}"}], ",", " ", 
      RowBox[{"{", "_", "}"}], ",", " ", 
      RowBox[{"{", 
       RowBox[{"incr", " ", "=", " ", 
