Cell[TextData[{
 StyleBox["NestWhile", "Literal"],
 " and ",
 StyleBox["NestWhileList", "Literal"],
 " generalize ",
 StyleBox["Nest", "Literal"],
 " and ",
 StyleBox["NestList", "Literal"],
 ", respectively, by adding a test predicate to determine if the iterative \
application of the function should continue. In addition to the test, an \
upper limit can be specified to guarantee the iteration terminates in a given \
number of steps if the test does not terminate it first. Here is an \
application that searches for a tour in a traveling salesperson problem (",
 StyleBox["TSP", "Techical Italic"],
 ") that is less than some specified distance. The cities are numbered 1 \
through ",
 StyleBox["n", "Literal"],
 ", and the distances are represented as a sparse matrix. "
}], "Text",
 CellChangeTimes->{{3.469986746545486*^9, 3.469986777933773*^9}, 
   3.4704890448293858`*^9, {3.4754146851125917`*^9, 3.4754146917514877`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"Make", " ", "random", " ", "set", " ", "of", " ", 
    RowBox[{"cities", "."}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"makeCities", "[", "n_", "]"}], ":=", "\[IndentingNewLine]", 
    RowBox[{"SparseArray", "[", 
     RowBox[{"Flatten", "[", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"i", ",", "j"}], "}"}], "\[Rule]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"i", "\[Equal]", "j"}], ",", "0", ",", 
           RowBox[{"RandomReal", "[", 
            RowBox[{"{", 
             RowBox[{"1", ",", "50"}], "}"}], "]"}]}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "n"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "i"}], "}"}]}], "]"}], "]"}], "]"}]}], 
   "\n", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"Given", " ", "set", " ", "of", " ", "cities"}], ",", " ", 
     RowBox[{"and", " ", "two", " ", "particular", " ", "cities"}], ",", " ", 
     
     RowBox[{"return", " ", "distance", " ", 
      RowBox[{"between", "."}]}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"distance", "[", 
     RowBox[{"cities_", ",", "c1_", ",", "c2_"}], "]"}], " ", ":=", " ", 
    "\[IndentingNewLine]", 
    RowBox[{"With", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"i1", "=", 
         RowBox[{"Max", "[", 
          RowBox[{"c1", ",", "c2"}], "]"}]}], ",", 
        RowBox[{"i2", "=", 
         RowBox[{"Min", "[", 
          RowBox[{"c1", ",", "c2"}], "]"}]}]}], "}"}], ",", 
      RowBox[{"cities", "[", 
       RowBox[{"[", 
        RowBox[{"i1", ",", "i2"}], "]"}], "]"}]}], "]"}]}], "\n", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"Given", " ", "a", " ", "tour"}], ",", " ", 
     RowBox[{
     "compute", " ", "the", " ", "total", " ", "distance", " ", "traveled", 
      " ", "if", " ", "you", " ", "visit", " ", "each", " ", "city", " ", 
      "and", " ", "return", " ", "to", " ", "the", " ", 
      RowBox[{"first", "."}]}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"totalDistance", "[", 
     RowBox[{"cities_", ",", "tour_"}], "]"}], " ", ":=", " ", 
    "\[IndentingNewLine]", 
    RowBox[{"Total", "[", 
     RowBox[{"ListConvolve", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"1", ",", "1"}], "}"}], ",", "tour", ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"-", "1"}], ",", 
         RowBox[{"-", "1"}]}], "}"}], ",", "tour", ",", 
       RowBox[{"#2", "&"}], ",", 
       RowBox[{
        RowBox[{"distance", "[", 
         RowBox[{"cities", ",", "#1", ",", "#2"}], "]"}], "&"}]}], "]"}], 
     "]"}]}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Make", " ", "an", " ", "initial", " ", "tour", " ", "where", " ", 
     "cities", " ", "are", " ", "visted", " ", "in", " ", "ascending", " ", 
     "order", " ", "of", " ", "city", " ", 
     RowBox[{"number", "."}]}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"makeOrderedTour", "[", "cities_", "]"}], " ", ":=", " ", 
    RowBox[{"Range", "[", 
     RowBox[{"Length", "[", "cities", "]"}], "]"}]}], "\n", 
   RowBox[{"(*", 
    RowBox[{
    "Randomly", " ", "sample", " ", "tours", " ", "until", " ", "a", " ", 
     "tour", " ", "is", " ", "less", " ", "than", " ", "specified", " ", 
     "distance", " ", "or", " ", "maxTries", " ", "is", " ", 
     RowBox[{"exceeded", "."}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"findTourLessThan", "[", 
     RowBox[{"cities_", ",", "distance_", ",", "maxTries_"}], "]"}], " ", ":=",
     " ", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"n", "=", 
        RowBox[{"Length", "[", "cities", "]"}]}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"NestWhile", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"RandomSample", "[", 
          RowBox[{"#", ",", "n"}], "]"}], "&"}], ",", 
        RowBox[{"makeOrderedTour", "[", "cities", "]"}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{"totalDistance", "[", 
           RowBox[{"cities", ",", "#"}], "]"}], " ", ">=", " ", "distance"}], 
         "&"}], ",", "maxTries"}], "]"}]}], "]"}]}]}]}]], "Input",
 CellChangeTimes->{{3.469986784693212*^9, 3.469986858934668*^9}},
 CellLabel->"In[170]:="],

