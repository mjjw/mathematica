   RowBox[{
   "principal_", ",", " ", "rate_", ",", " ", "years_", ",", " ", "n_"}], 
   "]"}], " ", ":=", 
  RowBox[{"Nest", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"#", 
      RowBox[{"(", 
       RowBox[{"1.0", "+", 
        RowBox[{"rate", "/", "n"}]}], ")"}]}], "&"}], ",", "principal", ",", 
