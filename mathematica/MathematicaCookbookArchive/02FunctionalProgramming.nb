Cell[TextData[{
 "Create a symbol called ",
 StyleBox["closure", "Literal"],
 " with attributes ",
 StyleBox["HoldAll", "Literal"],
 " and with the form ",
 StyleBox["closure [var_List, val_List, func_List]", "Literal"],
 ". Create an evaluation function for closures that executes in a private \
environment provided by ",
 StyleBox["Block", "Literal"],
 " and returns the result and a new closure that captures any state changes \
that occurred during the evaluation."
}], "Text",
 DefaultNewCellStyle->"Input",
 CellChangeTimes->{{3.469989116846779*^9, 3.469989125766347*^9}, 
   3.4754150027965117`*^9}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"SetAttributes", "[", 
   RowBox[{"closure", ",", "HoldAll"}], "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"SetAttributes", "[", 
   RowBox[{"evaluate", ",", "HoldFirst"}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"evaluate", "[", 
   RowBox[{"f_", ",", " ", 
    RowBox[{"closure", "[", 
     RowBox[{"vars_", ",", "vals_", ",", "funcs_"}], "]"}]}], "]"}], " ", ":=",
   " ", 
  RowBox[{"Block", "[", 
   RowBox[{"vars", ",", " ", 
    RowBox[{
     RowBox[{"vars", " ", "=", " ", "vals"}], ";", 
     RowBox[{"Block", "[", 
      RowBox[{"funcs", ",", " ", 
       RowBox[{"{", 
        RowBox[{"f", " ", ",", 
