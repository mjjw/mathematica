  RowBox[{"Clear", "[", "ff", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"MapIndexed", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"ff", "[", 
     RowBox[{"#1", ",", 
      RowBox[{"First", "[", "#2", "]"}]}], "]"}], "&"}], ",", " ", 
   RowBox[{"{", 
    RowBox[{"a", ",", "b", ",", "c", ",", "d", ",", "e"}], "}"}]}], 
