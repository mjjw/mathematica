Cell["HoldRest", "Heading3"],

Cell[TextData[{
 "This is the opposite of ",
 StyleBox["HoldFirst", "Literal"],
 "; the first argument is evaluated normally, but all",
 " ",
 "remaining arguments are kept in unevaluated form."
}], "Text",
 CellChangeTimes->{{3.4699032849977703`*^9, 3.4699033033099546`*^9}, 
   3.4700557225606813`*^9, {3.470156640164721*^9, 3.470156641925201*^9}, 
   3.4855222797332907`*^9}]
}, Open  ]],

